@model List<Sector>
<div class="content-page">
    <div class="content">
        <div class="container-fluid mt-4">
            <div class="row justify-content-center">
                <div class="col-md-12">
                   


                            <div class="table-responsive mt-4"></div>
                                <table class="table table-hover table-striped">
                                    <thead>
                                        <tr class="bg-secondary text-white">
                                            <th>Id</th>
                                            <th>Nombres</th>
                                            <th>Descripcion</th>
                                            <th>Autor</th>
                                            <th>created_at</th>
                                            <th>updated_at</th>
                                           
                                            
                                        </tr>
                                    </thead>
                                    @foreach (var sector in Model)
                                    {
                                        <tr>
                                            <td>@sector.Id</td>
                                            <td>@sector.Name</td>
                                            <td>@sector.Description</td>
                                            <td>@sector.Author</td>
                                            <td>@sector.created_at</td>
                                            <td>@sector.updated_at</td>
                                            

                                            
                                            
                                        </tr>
                                    }

                                </table>
                          
                </div>
            </div>
        </div>
    </div>
</div>
